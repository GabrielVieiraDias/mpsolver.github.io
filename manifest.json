{
  "manifest_version": 2,
  "name": "MathProg Solver",
  "version": "0.1",
  "description": "A Chrome application to edit and solve MathProg models.",
  "icons": {
    "128": "images/icon_128.png"
  },
  "permissions": [
	  {"fileSystem": ["write","directory"]}
  ],
  "app": {
    "background": {
      "scripts": ["background.js"]
    }
  },
  "minimum_chrome_version": "28"
}